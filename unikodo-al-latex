#!/usr/bin/perl

while (<>) {
    s/\306/^C/g;
    s/\346/^c/g;
    s/\330/^G/g;
    s/\370/^g/g;
    s/\246/^H/g;
    s/\266/^h/g;
    s/\254/^J/g;
    s/\274/^j/g;
    s/\336/^S/g;
    s/\376/^s/g;
    s/\335/^U/g;
    s/\375/^u/g;

    s/\304\210/^C/g;
    s/\304\211/^c/g;
    s/\304\234/^G/g;
    s/\304\235/^g/g;
    s/\304\244/^H/g;
    s/\304\245/^h/g;
    s/\304\264/^J/g;
    s/\304\265/^j/g;
    s/\305\234/^S/g;
    s/\305\235/^s/g;
    s/\305\254/^U/g;
    s/\305\255/^u/g;
    
    s/&#264;/^C/g;
    s/&#265;/^c/g;
    s/&#284;/^G/g;
    s/&#285;/^g/g;
    s/\304\244/^H/g;
    s/\304\245/^h/g;
    s/&#308;/^J/g;
    s/&#309;/^j/g;
    s/&#348;/^S/g;
    s/&#349;/^s/g;
    s/&#364;/^U/g;
    s/&#365;/^u/g;
    print;
};
