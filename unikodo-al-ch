#!/usr/bin/perl

while (<>) {
    s/\306/Ch/g;
    s/\346/ch/g;
    s/\330/Gh/g;
    s/\370/gh/g;
    s/\246/K/g;
    s/\266/k/g;
    s/\254/Jh/g;
    s/\274/jh/g;
    s/\336/Sh/g;
    s/\376/sh/g;
    s/\335/U/g;
    s/\375/u/g;

    s/\304\210/Ch/g;
    s/\304\211/ch/g;
    s/\304\234/Gh/g;
    s/\304\235/gh/g;
    s/\304\244/K/g;
    s/\304\245/k/g;
    s/\304\264/Jh/g;
    s/\304\265/jh/g;
    s/\305\234/Sh/g;
    s/\305\235/sh/g;
    s/\305\254/U/g;
    s/\305\255/u/g;
    
    s/&#264;/Ch/g;
    s/&#265;/ch/g;
    s/&#284;/Gh/g;
    s/&#285;/gh/g;
    s/\304\244/K/g;
    s/\304\245/k/g;
    s/&#308;/Jh/g;
    s/&#309;/jh/g;
    s/&#348;/Sh/g;
    s/&#349;/sh/g;
    s/&#364;/U/g;
    s/&#365;/u/g;
    print;
};
